{
  "apps" : [ {
    "name"        : "keystone",
    "script"      : "./keystone.js",
    "watch": "./",
    "instances": "4",
    "exec_mode": "cluester",
    "max_memory_restart": "100M",
    "env": {
      "NODE_ENV": "development"
    },
    "env_production" : {
       "NODE_ENV": "production"
    }
  } ]
}
